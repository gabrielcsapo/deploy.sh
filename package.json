    "async": "^2.1.2",
    "basic-auth-connect": "^1.0.0",
    "bluebird": "^3.4.1",
    "body-parser": "^1.15.2",
    "bunyan": "^1.8.1",
    "chart.js": "^2.3.0",
    "commander": "^2.9.0",
    "cross-spawn": "^5.0.1",
    "express": "^4.14.0",
    "geoip-lite": "^1.1.8",
    "git-server": "git+https://github.com/stackdot/NodeJS-Git-Server.git",
