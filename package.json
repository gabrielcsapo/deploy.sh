{
  "name": "node-distribute",
  "version": "0.7.1",
  "description": "continuous deployment of node services, make your own vpn ☁️",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/lessc ./operations/views/admin/admin.less ./operations/views/admin/dist/admin.css; pug operations/views/404/index.pug; webpack;",
    "test": "npm run test:functional; npm run test:unit",
    "test:functional": "npm run lint;redis-cli flushall;./scripts/functional-test.sh;",
    "test:unit": "npm run coverage;",
    "lint": "eslint --ext js,vue .",
    "start": "node index.js | ./node_modules/.bin/bunyan -o short",
    "coverage": "./scripts/unit-test.sh; nyc --reporter=html mocha test/unit/index.js",
    "coveralls": "./scripts/unit-test.sh; nyc mocha test/unit/index.js && nyc report --reporter=text-lcov | coveralls",
    "postinstall": "rm -rf node_modules/webpack/node_modules/uglify-js",
    "generate-docs": "node_modules/.bin/jsdoc -c jsdoc.json"
  },
  "bin": {
    "node-distribute": "bin/node-distribute"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielcsapo/node-distribute.git"
  },
  "author": "Gabriel J. Csapo <gabecsapo@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gabrielcsapo/node-distribute/issues"
  },
  "homepage": "https://github.com/gabrielcsapo/node-distribute#readme",
  "dependencies": {
    "async": "^2.1.2",
    "basic-auth-connect": "^1.0.0",
    "body-parser": "^1.15.2",
    "bunyan": "^1.8.1",
    "commander": "^2.9.0",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "geoip-lite": "^1.1.8",
    "git-server": "git+https://github.com/stackdot/NodeJS-Git-Server.git",
    "kue": "^0.11.5",
    "moment": "^2.14.1",
    "node-flat-db": "^0.2.1",
    "npm": "^4.0.2",
    "pm2": "^2.1.4",
    "portfinder": "^1.0.6",
    "psychic-ui": "^0.4.0",
    "request": "^2.79.0",
    "response-time": "^2.3.1",
    "rimraf": "^2.5.4",
    "socket.io": "^1.5.1",
    "ua-parser-js": "^0.7.12",
    "underscore": "^1.8.3",
    "vue": "^2.1.4"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "chance": "^1.0.4",
    "chart.js": "^2.4.0",
    "coveralls": "^2.11.15",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-plugin-html": "^1.7.0",
    "jsdoc": "^3.4.3",
    "less": "^2.7.1",
    "minami": "^1.1.1",
    "mocha": "^3.0.2",
    "nyc": "^10.0.0",
    "pug-cli": "^1.0.0-alpha6",
    "random-ua": "0.0.6",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.0",
    "uglify-js": "git://github.com/mishoo/UglifyJS2#harmony",
    "vue-html-loader": "^1.0.0",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.1.4",
    "webpack": "^2.1.0-beta.27"
  }
}
