{
  "name": "node-distribute",
  "version": "0.7.1",
  "description": "continuous deployment of node services, make your own vpn ☁️",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/lessc ./operations/views/admin/admin.less ./operations/views/admin/dist/admin.css; webpack;",
    "test": "npm run lint;npm run clean;./scripts/functional-test.sh;npm run coverage;",
    "lint": "eslint .",
    "start": "node index.js | ./node_modules/.bin/bunyan -o short",
    "clean": "redis-cli flushall;",
    "coverage": "./scripts/unit-test.sh; nyc --reporter=html mocha test/unit/index.js",
    "coveralls": "./scripts/unit-test.sh; nyc mocha test/unit/index.js && nyc report --reporter=text-lcov | coveralls"
  },
  "bin": {
    "node-distribute": "bin/node-distribute"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielcsapo/node-distribute.git"
  },
